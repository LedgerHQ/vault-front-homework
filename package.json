    "format": "prettier --config .prettierrc --write src/**/*.{ts,tsx}"
  },
  "dependencies": {
    "axios": "0.27.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "5.3.5",
    "swr": "1.3.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "eslint": "8.14.0",
    "parcel": "^2.0.0-beta.2",
    "prettier": "2.6.2",
    "typescript": "^4.2.4"
  },
  "alias": {
